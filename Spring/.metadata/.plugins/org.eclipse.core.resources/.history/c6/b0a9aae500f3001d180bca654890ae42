package org.jsp.UsingValue;

import org.springframework.beans.factory.annotation.Value;

public class Person {
	@Value(value="26")
private int age;
private String Name;
private long phone;
public Person(@Value(value = "24")long phone) {
	this.phone=phone;
}
@Value(value="Pupu")
public String setName(@Value(value="sumit")String Name)
{
	return Name;
}

}
